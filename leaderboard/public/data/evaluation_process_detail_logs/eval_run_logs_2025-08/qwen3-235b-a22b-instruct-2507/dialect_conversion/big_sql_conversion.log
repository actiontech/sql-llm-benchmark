               KK.PANAMA_CANAL_NET_TON, 
               KK.HOST_POWER, 
               KK.CONSTRUCTION_DATE, 
               KK.CREATE_YEAR, 
               KK.OPERATION_DATE, 
               KK.RETIRED_DATE, 
               KK.CALL_SIGN, 
               KK.IMO_NO
        FROM (
            SELECT ROW_NUMBER() OVER (ORDER BY MB.VESSEL_CODE) AS ROW_NO,
                   MB.VESSEL_CODE,
                   MB.VESSEL_NAME,
                   MB.VESSEL_NAME_EN,
                   MB.ANOTHER_NAME,
