                         AND c.code_value = MB.ship_owner_flag 
                       LIMIT 1
                   )) AS SHIP_OWNER_FLAG_NAME,
                   MB.VESSEL_TYPE,
                   COALESCE(MB.AREA, (
                       SELECT C.DISPLAY_VALUE_CN 
                       FROM CDM_CODEDICT C 
                       WHERE C.CODE_TYPE = 'CDM_NAVIGATING_ZONE' 
                         AND C.CODE_VALUE = MB.AREA
