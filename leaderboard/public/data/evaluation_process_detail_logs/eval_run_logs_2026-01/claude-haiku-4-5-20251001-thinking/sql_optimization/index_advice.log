  KEY `ix_hotel_price_adjustment_08` (`CREATE_USER_ID`,`create_time`,`apply_id`)
) ENGINE=InnoDB AUTO_INCREMENT=9143968 DEFAULT CHARSET=utf8mb4 COMMENT='调价-审批流程主表';

